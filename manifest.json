{
  "name": "Github Label Migrator",
  "version": "1.0",
  "description": "Takes issue labels from one github repo and migrates it to another",
  "permissions": ["activeTab", "declarativeContent", "storage", "https://github.com/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png",
      "128": "images/favicon-128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*/labels"],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "128": "images/favicon-128x128.png"
  },
  "manifest_version": 2
}